{
  "name": "neuropacs",
  "version": "1.1.9",
  "description": "Connect to NeuroPACS diagnostic capabilities with our Nodejs SDK.",
  "main": "dist/neuropacs.js",
  "scripts": {
    "build": "npx babel src -d dist",
    "test": "jest --config package.json",
    "prepare": "npm run build"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
