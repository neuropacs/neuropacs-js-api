  "bugs": {
    "url": "https://github.com/neuropacs/neuropacs-js-sdk/issues"
  },
  "homepage": "https://github.com/neuropacs/neuropacs-js-sdk#readme",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "cross-fetch": "^4.0.0",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "jsdom": "^24.1.0",
    "node-fetch": "^3.3.2",
    "util": "^0.12.5",
    "uuid": "^10.0.0"
